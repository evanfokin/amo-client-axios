{
  "name": "amo-client-axios",
  "version": "0.0.4",
  "main": "dist/index.js",
  "author": {
    "name": "Ivan Fokin"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc --build",
    "build:watch": "nodemon --exec yarn run build",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "yarn run format && yarn run build"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "husky": "^8.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "bottleneck": "^2.19.5",
    "lodash": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run prepare"
    }
  }
}
