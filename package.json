{
  "name": "amo-client-axios",
  "version": "0.0.3",
  "main": "dist/index.js",
  "author": {
    "name": "Ivan Fokin"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc --build",
    "build:watch": "nodemon --exec yarn run build",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "yarn run format && yarn run build"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.168",
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bottleneck": "^2.19.5",
    "lodash": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run prepare"
    }
  }
}
